entity PROGRAMACION{
	NUMPARTIDO : VARCHAR2
	FECHA : DATE
	ID_LUGAR : VARCHAR2
	ID_FASE : VARCHAR2
	ID_GRUPO : VARCHAR2
	ID_EQUIPO : VARCHAR2
	EQU_ID_GRUPO : VARCHAR2
	EQU_ID_EQUIPO : VARCHAR2
}

entity ESTADIO{
	ID_LUGAR : VARCHAR2
	NOMBRE_ESTADIO : VARCHAR2
}

entity GRUPO{
	ID_GRUPO : VARCHAR2
}

entity EQUIPO{
	ID_EQUIPO : VARCHAR2
	NOMBRE_EQUIPO : VARCHAR2
}


